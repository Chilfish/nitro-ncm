{
  "name": "hono-ncm",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "author": {
    "name": "Chilfish",
    "email": "chill4fish@gmail.com",
    "url": "https://chilfish.top"
  },
  "license": "MIT",
  "homepage": "https://github.com/Chilfish/hono-ncm.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chilfish/hono-ncm.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch --no-cache src/node.ts",
    "build": "tsx scripts/build.ts",
    "start": "tsx src/node.ts",
    "prepare": "husky || true && pnpm build",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^1.6.7",
    "axios-cache-interceptor": "^1.5.1",
    "hono": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.2",
    "@hono/node-server": "^1.8.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.27",
    "eslint": "npm:eslint-ts-patch@8.57.0-0",
    "eslint-ts-patch": "8.57.0-0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "tsx": "^4.7.1",
    "unbuild": "^2.0.0",
    "unplugin-auto-import": "^0.17.5"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
